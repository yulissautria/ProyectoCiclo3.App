@page
@model ProyectoCiclo3.App.Frontend.Pages.EditUsuarioModel
@{
    var usuario = Model.Usuario;
}
 
<div class="text-center">
<form method="post">
  
  @*<label for="id" class="form-label">id:</label>*@
  <input type="number" class="form-control d-none" id="id" name="id" placeholder="id" asp-for="@usuario.id">

  <label for="Nombres" class="form-label">Nombres:</label>
  <input type="text" class="form-control" id="nombres" name="nombres" placeholder="Nombres" asp-for="@usuario.nombres">

  <label for="Apellidos" class="form-label">Apellidos:</label>
  <input type="text" class="form-control" id="apellidos" name="apellidos" placeholder="Apellidos" asp-for="@usuario.apellidos">

  <label for="Dirección" class="form-label">Dirección:</label>
  <input type="text" class="form-control" id="dirección" name="dirección" placeholder="Dirección" asp-for="@usuario.dirección">
  
  <label for="Teléfono" class="form-label">Teléfono:</label>
  <input type="text" class="form-control" id="teléfono" name="teléfono" placeholder="Teléfono" asp-for="@usuario.teléfono">
  
  
  <hr>
  <button type="submit" class="btn btn-success">Enviar</button>
</form>
</div>
